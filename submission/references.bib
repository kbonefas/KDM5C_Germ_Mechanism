@article{weiPatientMutationsIntellectual2016,
  type = {Journal Article},
  title = {Patient {{Mutations}} of the {{Intellectual Disability Gene KDM5C Downregulate Netrin G2}} and {{Suppress Neurite Growth}} in {{Neuro2a Cells}}},
  author = {Wei, G. and Deng, X. and Agarwal, S. and Iwase, S. and Disteche, C. and Xu, J.},
  date = {2016},
  journaltitle = {J Mol Neurosci},
  volume = {60},
