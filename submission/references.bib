@article{bonefasSexuallyDimorphicAlterations2023,
  title = {Sexually {{Dimorphic Alterations}} in the {{Transcriptome}} and {{Behavior}} with {{Loss}} of {{Histone Demethylase KDM5C}}},
  author = {Bonefas, Katherine M. and Vallianatos, Christina N. and Raines, Brynne and Tronson, Natalie C. and Iwase, Shigeki},
  date = {2023-02-16},
  journaltitle = {Cells},
