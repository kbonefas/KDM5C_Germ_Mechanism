@article{limNuageMediatesRetrotransposon2013,
  title = {The Nuage Mediates Retrotransposon Silencing in Mouse Primordial Ovarian Follicles},
  author = {Lim, Ai Khim and Lorthongpanich, Chanchao and Chew, Ting Gang and Tan, Chin Wee Godwin and Shue, Yan Ting and Balu, Sathish and Gounko, Natalia and Kuramochi-Miyagawa, Satomi and Matzuk, Martin M. and Chuma, Shinichiro and Messerschmidt, Daniel M. and Solter, Davor and Knowles, Barbara B.},
