@article{defelipeAlteredNociceptionAnalgesia1998,
  type = {Journal Article},
  title = {Altered Nociception, Analgesia and Aggression in Mice Lacking the Receptor for Substance {{P}}},
  author = {De Felipe, C. and Herrero, J. F. and O'Brien, J. A. and Palmer, J. A. and Doyle, C. A. and Smith, A. J. and Laird, J. M. and Belmonte, C. and Cervero, F. and Hunt, S. P.},
  date = {1998},
  journaltitle = {Nature},
