@article{coschieraPrimaryCiliaPromote2024,
  title = {Primary Cilia Promote the Differentiation of Human Neurons through the {{WNT}} Signaling Pathway},
  author = {Coschiera, Andrea and Yoshihara, Masahito and Lauter, Gilbert and Ezer, Sini and Pucci, Mariangela and Li, Haonan and Kavšek, Alan and Riedel, Christian G. and Kere, Juha and Swoboda, Peter},
  date = {2024-02-27},
