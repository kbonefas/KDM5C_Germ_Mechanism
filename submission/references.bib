@article{kreipkeDifferentialEffectsEndothelin2010,
  type = {Journal Article},
  title = {Differential Effects of Endothelin Receptor {{A}} and {{B}} Antagonism on Cerebral Hypoperfusion Following Traumatic Brain Injury},
  author = {Kreipke, C. W. and Schafer, P. C. and Rossi, N. F. and Rafols, J. A.},
  date = {2010},
  journaltitle = {Neurol Res},
  volume = {32},
  number = {2},
  eprint = {19570328},
  eprinttype = {pubmed},
  pages = {209--14},
  issn = {1743-1328},
  doi = {10.1179/174313209X414515},
  url = {https://www.ncbi.nlm.nih.gov/pubmed/19570328},
  abstract = {Our laboratory has previously shown that endothelin 1 (ET-1), a powerful vasoconstrictor, and its receptors, A (ETrA) and B (ETrB), are up-regulated following trauma. This up-regulation coincides temporally with enhanced vasoreactivity in cerebral cortical microvessels, which leads to a state of chronic hypoperfusion for up to 48 hours following traumatic brain injury (TBI). However, the direct contribution of either receptor up-regulation to decreased cerebral blood flow (CBF) after closed head trauma has not been determined. Furthermore, how ET-1 blockade may affect histological outcome following TBI has not been explored. Therefore, the effects of ETrA and B antagonism on TBI induced hypoperfusion of CBF and cell injury in sensorimotor cortex (smCx) and hippocampus (Hipp) were assessed by arterial spin labeling magnetic resonance imaging and Fluoro-Jade staining, respectively.|Adult male rats were given intracerebroventricular injections of ETrA (BQ123) or ETrB antagonist (BQ788) before being subjected to TBI using a closed head acceleration impact model. Following TBI, CBF was measured and histological examination of cell integrity was carried out.|ETrA blockade ameliorated TBI induced hypoperfusion in smCx and Hipp at 4 and 24 hours after TBI and caused a mild hyperemia in both centers by 48 hours after injury. Furthermore, ETrA antagonism reduced the extent of Fluoro-Jade labeled cells within smCx and Hipp as compared with TBI only. ETrB blockade had little effect on TBI induced hypoperfusion and did not change the extent of cell injury following TBI.|These results suggest that decreased CBF following TBI may be caused by ETrA, but not ETrB, up-regulation. Furthermore, these results suggest that TBI induced hypoperfusion may contribute to poor neurologic outcome following TBI. In this work, we provide a rationale for studying the clinical relevancy of use of ETrA antagonists following TBI.},
  keywords = {Animals Brain Injuries Endothelin A Receptor Antagonists Endothelin B Receptor Antagonists Male Motor Cortex Peptides,Cyclic Rats Rats,Endothelin A Receptor,Endothelin B,Sprague-Dawley Receptor}
}

@article{krobertSpontaneousAmphetamineevokedRelease1994,
  type = {Journal Article},
