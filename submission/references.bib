@article{chongRESTMammalianSilencer1995,
  type = {Journal Article},
  title = {{{REST}}: A Mammalian Silencer Protein That Restricts Sodium Channel Gene Expression to Neurons},
  author = {Chong, J. A. and Tapia-Ramírez, J. and Kim, S. and Toledo-Aral, J. J. and Zheng, Y. and Boutros, M. C. and Altshuller, Y. M. and Frohman, M. A. and Kraner, S. D. and Mandel, G.},
