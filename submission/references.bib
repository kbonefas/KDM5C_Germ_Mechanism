@article{vinogradovaInhibitorKDM5Demethylases2016,
  type = {Journal Article},
  title = {An Inhibitor of {{KDM5}} Demethylases Reduces Survival of Drug-Tolerant Cancer Cells},
  author = {Vinogradova, M. and Gehling, V. S. and Gustafson, A. and Arora, S. and Tindell, C. A. and Wilson, C. and Williamson, K. E. and Guler, G. D. and Gangurde, P. and Manieri, W. and Busby, J. and Flynn, E. M. and Lan, F. and Kim, H. J. and Odate, S. and Cochran, A. G. and Liu, Y. and Wongchenko, M. and Yang, Y. and Cheung, T. K. and Maile, T. M. and Lau, T. and Costa, M. and Hegde, G. V. and Jackson, E. and Pitti, R. and Arnott, D. and Bailey, C. and Bellon, S. and Cummings, R. T. and Albrecht, B. K. and Harmange, J. C. and Kiefer, J. R. and Trojer, P. and Classon, M.},
