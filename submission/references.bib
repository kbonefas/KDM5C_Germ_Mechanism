@article{wulfridgeGquadruplexesAssociatedRloops2023,
  title = {G-Quadruplexes Associated with {{R-loops}} Promote {{CTCF}} Binding},
  author = {Wulfridge, Phillip and Yan, Qingqing and Rell, Nathaniel and Doherty, John and Jacobson, Skye and Offley, Sarah and Deliard, Sandra and Feng, Kelly and Phillips-Cremins, Jennifer E. and Gardini, Alessandro and Sarma, Kavitha},
