@article{shenSexuallyDimorphicRNA2022,
  title = {Sexually Dimorphic {{RNA}} Helicases {{DDX3X}} and {{DDX3Y}} Differentially Regulate {{RNA}} Metabolism through Phase Separation},
  author = {Shen, Hui and Yanas, Amber and Owens, Michael C. and Zhang, Celia and Fritsch, Clark and Fare, Charlotte M. and Copley, Katie E. and Shorter, James and Goldman, Yale E. and Liu, Kathy Fange},
