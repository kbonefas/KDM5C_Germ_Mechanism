@article{liuStructuralBasisAbility2018,
  type = {Journal Article},
  title = {Structural Basis for the Ability of {{MBD}} Domains to Bind Methyl-{{CG}} and {{TG}} Sites in {{DNA}}},
  author = {Liu, K. and Xu, C. and Lei, M. and Yang, A. and Loppnau, P. and Hughes, T. R. and Min, J.},
