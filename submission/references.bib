}

@article{tyssowskiDifferentNeuronalActivity2018,
  type = {Journal Article},
  title = {Different {{Neuronal Activity Patterns Induce Different Gene Expression Programs}}},
  author = {Tyssowski, K. M. and DeStefino, N. R. and Cho, J. H. and Dunn, C. J. and Poston, R. G. and Carty, C. E. and Jones, R. D. and Chang, S. M. and Romeo, P. and Wurzelmann, M. K. and Ward, J. M. and Andermann, M. L. and Saha, R. N. and Dudek, S. M. and Gray, J. M.},
