@article{vallianatosDisruptedIntricacyHistone2015,
  type = {Journal Article},
  title = {Disrupted Intricacy of Histone {{H3K4}} Methylation in Neurodevelopmental Disorders},
  author = {Vallianatos, C. N. and Iwase, S.},
  date = {2015},
  journaltitle = {Epigenomics},
