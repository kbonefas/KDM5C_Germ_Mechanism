  author = {Zhang, Hui and Li, Jiaming and Yu, Yang and Ren, Jie and Liu, Qiang and Bao, Zhaoshi and Sun, Shuhui and Liu, Xiaoqian and Ma, Shuai and Liu, Zunpeng and Yan, Kaowen and Wu, Zeming and Fan, Yanling and Sun, Xiaoyan and Zhang, Yixin and Ji, Qianzhao and Cheng, Fang and Wei, Peng-Hu and Ma, Xibo and Zhang, Shiqiang and Xie, Zhengwei and Niu, Yuyu and Wang, Yan-Jiang and Han, Jing-Dong J. and Jiang, Tao and Zhao, Guoguang and Ji, Weizhi and Izpisua Belmonte, Juan Carlos and Wang, Si and Qu, Jing and Zhang, Weiqi and Liu, Guang-Hui},
