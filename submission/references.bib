@article{roselli-rehfussIdentificationReceptorGamma1993,
  type = {Journal Article},
  title = {Identification of a Receptor for Gamma Melanotropin and Other Proopiomelanocortin Peptides in the Hypothalamus and Limbic System},
  author = {Roselli-Rehfuss, L. and Mountjoy, K. G. and Robbins, L. S. and Mortrud, M. T. and Low, M. J. and Tatro, J. B. and Entwistle, M. L. and Simerly, R. B. and Cone, R. D.},
  date = {1993},
  journaltitle = {Proc Natl Acad Sci U S A},
  volume = {90},
