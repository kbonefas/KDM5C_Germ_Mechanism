@article{volpeRegulationHeterochromaticSilencing2002,
  type = {Journal Article},
  title = {Regulation of Heterochromatic Silencing and Histone {{H3}} Lysine-9 Methylation by {{RNAi}}},
  author = {Volpe, T. A. and Kidner, C. and Hall, I. M. and Teng, G. and Grewal, S. I. and Martienssen, R. A.},
