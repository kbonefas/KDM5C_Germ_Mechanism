  author = {Course, Meredith M and Gudsnuk, Kathryn and Keene, C Dirk and Bird, Thomas D and Jayadev, Suman and Valdmanis, Paul N},
  date = {2023-02-13},
  journaltitle = {Brain},
  volume = {146},
