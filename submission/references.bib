@article{wrennGalaninPeptideLevels2002,
  type = {Journal Article},
  title = {Galanin Peptide Levels in Hippocampus and Cortex of Galanin-Overexpressing Transgenic Mice Evaluated for Cognitive Performance},
  author = {Wrenn, C. C. and Marriott, L. K. and Kinney, J. W. and Holmes, A. and Wenk, G. L. and Crawley, J. N.},
