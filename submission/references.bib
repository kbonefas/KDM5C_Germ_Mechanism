@article{waynerRoleAngiotensinII1993,
  type = {Journal Article},
  title = {Role of Angiotensin {{II}} and {{AT1}} Receptors in Hippocampal {{LTP}}},
  author = {Wayner, M. J. and Armstrong, D. L. and Polan-Curtain, J. L. and Denny, J. B.},
