  author = {Zhang, Z. and Sapiro, R. and Kapfhamer, D. and Bucan, M. and Bray, J. and Chennathukuzhi, V. and McNamara, P. and Curtis, A. and Zhang, M. and Blanchette-Mackie, E. J. and Strauss, J. F.},
