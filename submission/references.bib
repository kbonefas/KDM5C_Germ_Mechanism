@article{dahletE2F6InitiatesStable2021,
  title = {{{E2F6}} Initiates Stable Epigenetic Silencing of Germline Genes during Embryonic Development},
  author = {Dahlet, Thomas and Truss, Matthias and Frede, Ute and Al Adhami, Hala and Bardet, Anaïs F. and Dumas, Michael and Vallet, Judith and Chicher, Johana and Hammann, Philippe and Kottnik, Sarah and Hansen, Peter and Luz, Uschi and Alvarez, Gonzalo and Auclair, Ghislain and Hecht, Jochen and Robinson, Peter N. and Hagemeier, Christian and Weber, Michael},
