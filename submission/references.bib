@article{liUpregulationGapJunction2001,
  type = {Journal Article},
  title = {Upregulation of Gap Junction Connexin 32 with Epileptiform Activity in the Isolated Mouse Hippocampus},
  author = {Li, J. and Shen, H. and Naus, C. C. and Zhang, L. and Carlen, P. L.},
  date = {2001},
