@article{paulsenUseBruSeqBruChaseSeq2014,
  type = {Journal Article},
  title = {Use of {{Bru-Seq}} and {{BruChase-Seq}} for Genome-Wide Assessment of the Synthesis and Stability of {{RNA}}},
  author = {Paulsen, M. T. and Veloso, A. and Prasad, J. and Bedi, K. and Ljungman, E. A. and Magnuson, B. and Wilson, T. E. and Ljungman, M.},
  date = {2014},
  journaltitle = {Methods},
  volume = {67},
