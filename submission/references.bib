}

@article{chowdhuryArcArg3Interacts2006,
  type = {Journal Article},
  title = {Arc/{{Arg3}}.1 Interacts with the Endocytic Machinery to Regulate {{AMPA}} Receptor Trafficking},
  author = {Chowdhury, S. and Shepherd, J. D. and Okuno, H. and Lyford, G. and Petralia, R. S. and Plath, N. and Kuhl, D. and Huganir, R. L. and Worley, P. F.},
  date = {2006},
  journaltitle = {Neuron},
  volume = {52},
