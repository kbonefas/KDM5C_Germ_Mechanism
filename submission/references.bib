@article{carmignacFurtherDelineationFemale2020,
  type = {Journal Article},
  title = {Further Delineation of the Female Phenotype with {{KDM5C}} Disease Causing Variants: 19 New Individuals and Review of the Literature},
  author = {Carmignac, V. and Nambot, S. and Lehalle, D. and Callier, P. and Moortgat, S. and Benoit, V. and Ghoumid, J. and Delobel, B. and Smol, T. and Thuillier, C. and Zordan, C. and Naudion, S. and Bienvenu, T. and Touraine, R. and Ramond, F. and Zweier, C. and Reis, A. and Kraus, C. and Nizon, M. and Cogné, B. and Verloes, A. and Tran Mau-Them, F. and Sorlin, A. and Jouan, T. and Duffourd, Y. and Tisserant, E. and Philippe, C. and Vitobello, A. and Thevenon, J. and Faivre, L. and Thauvin-Robinet, C.},
