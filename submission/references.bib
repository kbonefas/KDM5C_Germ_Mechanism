  title = {Repression of Germline {{RNAi}} Pathways in Somatic Cells by Retinoblastoma Pathway Chromatin Complexes},
  author = {Wu, X. and Shi, Z. and Cui, M. and Han, M. and Ruvkun, G.},
  date = {2012},
  journaltitle = {PLoS Genet},
  volume = {8},
