@article{whyteEnhancerDecommissioningLSD12012,
  type = {Journal Article},
  title = {Enhancer Decommissioning by {{LSD1}} during Embryonic Stem Cell Differentiation},
  author = {Whyte, W. A. and Bilodeau, S. and Orlando, D. A. and Hoke, H. A. and Frampton, G. M. and Foster, C. T. and Cowley, S. M. and Young, R. A.},
