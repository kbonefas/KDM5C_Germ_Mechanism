@article{chambersProteincoupledReceptorUDPglucose2000,
  type = {Journal Article},
  title = {A {{G}} Protein-Coupled Receptor for {{UDP-glucose}}},
  author = {Chambers, J. K. and Macdonald, L. E. and Sarau, H. M. and Ames, R. S. and Freeman, K. and Foley, J. J. and Zhu, Y. and McLaughlin, M. M. and Murdock, P. and McMillan, L. and Trill, J. and Swift, A. and Aiyar, N. and Taylor, P. and Vawter, L. and Naheed, S. and Szekeres, P. and Hervieu, G. and Scott, C. and Watson, J. M. and Murphy, A. J. and Duzic, E. and Klein, C. and Bergsma, D. J. and Wilson, S. and Livi, G. P.},
  date = {2000},
  journaltitle = {J Biol Chem},
  volume = {275},
  number = {15},
